#common elements and non-common elemnts in Python

L1 = ['a','b','d']
L2 = ['a','b','d']
L3=[]

#Finding common elements in both L1 and L2 and appending to L3
for i in range(0,len(L1)):
    for j in range(0,len(L2)):
        if L1[i]==L2[j]:
            L3.append(L1[i])
            
#Removing elements from L1 that are present in L2
for i in range(0,len(L3)):
    for i in L1:
        if i in L3:
            L1.remove(i)
        
print (L3)
print (L1)
